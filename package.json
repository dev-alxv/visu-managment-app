{
  "name": "ff-frontend-app",
  "version": "1.0.0",
  "buildNum": "",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --configuration production && ncu",
    "buildDev": "ng build --configuration prodDev && ncu",
    "test": "ng test --source-map=false --code-coverage",
    "e2e:cypress": "cypress open",
    "test:pipeline": "ng test --source-map=false --watch=false --browsers=ChromeHeadless",
    "ci:test": "npm run test:pipeline",
    "ci:build-sandbox": "ng build --configuration prodDev --base-href /ff-ui/ --output-path dist/ff-ui",
    "ci:build": "ng build --configuration production --base-href /ff-ui/ --output-path dist/ff-ui",
    "ci:test-e2e": "npm run ci:build && ./node_modules/.bin/run-p --race ci:serve ci:cypress",
    "ci:serve": "http-server ./dist -a localhost -p 4200 -c-1",
    "ci:cypress": "npx cypress run --config video=false --browser chrome --headless",
    "ci:start": "npm run ci:test && npm run ci:test-e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/file-input": "^6.0.0",
    "@angular/animations": "~12.2.2",
    "@angular/cdk": "^12.2.2",
    "@angular/common": "~12.2.2",
    "@angular/compiler": "~12.2.2",
    "@angular/core": "~12.2.2",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.2.2",
    "@angular/localize": "^12.2.2",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "~12.2.2",
    "@angular/platform-browser-dynamic": "~12.2.2",
    "@angular/router": "~12.2.2",
    "@iplab/ngx-file-upload": "^3.3.1",
    "@material-ui/icons": "^4.11.2",
    "@ngneat/until-destroy": "^8.1.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "flag-icon-css": "^3.5.0",
    "mat-timepicker": "^5.1.4",
    "ngx-mat-timepicker": "^12.0.0",
    "ngx-pagination": "^5.1.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.2",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "~12.2.2",
    "@angular/compiler-cli": "~12.2.2",
    "@types/jasmine": "~3.8.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.18.2",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "cypress": "^8.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-prettier": "^3.4.0",
    "http-server": "^13.0.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "npm-check-updates": "^11.8.3",
    "typescript": "~4.3.5"
  }
